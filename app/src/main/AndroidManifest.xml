<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jbory_000.databazezamestnancu">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ZamestnanciListActivity"
            android:label="@string/title_zamestnanci_list"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.jbory_000.databazezamestnancu.MainActivity" />
        </activity>
        <activity
            android:name=".ZamestnanciDetailActivity"
            android:label="@string/title_zamestnanci_detail"
            android:parentActivityName=".ZamestnanciListActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.jbory_000.databazezamestnancu.ZamestnanciListActivity" />
        </activity>
        <activity
            android:name=".ProjektyListActivity"
            android:label="@string/title_projekty_list"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.jbory_000.databazezamestnancu.MainActivity" />
        </activity>
        <activity
            android:name=".ProjektyDetailActivity"
            android:label="@string/title_projekty_detail"
            android:parentActivityName=".ProjektyListActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.jbory_000.databazezamestnancu.ProjektyListActivity" />
        </activity>
        <activity android:name=".OAplikaci"></activity>
    </application>

</manifest>