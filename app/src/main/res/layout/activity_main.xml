<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.jbory_000.databazezamestnancu.MainActivity">


    <LinearLayout
        android:layout_width="368dp"
        android:layout_height="495dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="8dp"
        tools:ignore="MissingConstraints">

        <Button
            android:id="@+id/btnZamestnanci"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/zamestnanci"
            android:onClick="vyberMenu"/>

        <Button
            android:id="@+id/btnProjekty"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/projekty"
            android:onClick="vyberMenu"/>

        <Button
            android:id="@+id/btnOAplikaci"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/o_aplikaci"
            android:onClick="vyberMenu"/>
    </LinearLayout>
</android.support.constraint.ConstraintLayout>
